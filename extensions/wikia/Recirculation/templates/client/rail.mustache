<section class="recirculation-rail module">
	<h2>{{ title }}</h2>
	<ul class="thumbnails">
		{{#items}}
			<li class="title-thumbnail">
				<a href="{{ url }}" class="image image-thumbnail fluid small">
					<img src="{{ thumbnail }}" alt="{{ title }}" itemprop="thumbnail">
				</a>
				<div class="title" title="{{ title }}">
					<a href="{{ url }}">{{ title }}</a>
				</div>
			</li>
		{{/items}}
	</ul>
</section>
